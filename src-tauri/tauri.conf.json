{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "cheese-schedule",
  "version": "0.1.0",
  "identifier": "com.wanzii.cheese.schedule",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "cheese-schedule",
        "title": "奶酪课程表",
        "transparent": true,
        "alwaysOnBottom": true,
        "decorations": false,
        "width": 170,
        "height": 600
      },
      {
        "label": "editor",
        "title": "奶酪课程表编辑器",
        "url": "/#/editor"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}